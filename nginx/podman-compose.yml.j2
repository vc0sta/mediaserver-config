
version: '3'
services:
  nginx:
    image: nginx
    container_name: nginx
    ports:
      - "12080:80"
      - "12443:443"
    volumes:
      - ./nginx.conf:/etc/nginx/nginx.conf
      - ../Certbot-Godaddy/config:/etc/letsencrypt
    restart: unless-stopped

networks:
  default:
    external:
      name: {{ container_network_name }}
